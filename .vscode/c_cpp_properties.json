{
    "configurations": [
        {
            "name": "FreeRTOS",
            "includePath": [
                "${workspaceFolder}/device/inc",
                "${workspaceFolder}/freertos/include",
                "${workspaceFolder}/freertos/portable/GCC/ARM_CM7/r0p1",
                "${workspaceFolder}/cores/teensy4"
            ],
            "defines": [
                "F_CPU=600000000",
                "USB_SERIAL",
                "LAYOUT_US_ENGLISH",
                "USING_MAKEFILE",
                "__IMXRT1062__",
                "ARDUINO=10813",
                "TEENSYDUINO=159",
                "ARDUINO_TEENSY41",
                "PRINT_DEBUG_STUFF",
                "PRINT_DEBUG_USING_USB"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}